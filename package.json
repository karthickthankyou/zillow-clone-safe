{
  "name": "zillow-clone",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "buildci": "CI=true next build",
    "prestart": "npm run build",
    "start": "next start",
    "lint-next": "next lint",
    "lint": "eslint --ext .tsx,.ts . --fix",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "prettier": "prettier --ignore-path .gitignore \"**/*.{ts,tsx}\"",
    "format:check": "npm run prettier -- --check",
    "format:fix": "npm run prettier -- --write",
    "type:check": "tsc",
    "validate": "npm-run-all --parallel type:check format:check lint",
    "validate-nobuild": "npm-run-all --parallel type:check format:check lint",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook",
    "prepare": "husky install",
    "chromatic": "chromatic --exit-zero-on-changes",
    "preanalyze": "npm run build",
    "analyze": "npx source-map-explorer .next/static/**/*.js",
    "depcheck": "npx dependency-cruiser --config .dependency-cruiser.js src",
    "depcheck:graph": "npx dependency-cruiser --include-only 'src' --output-type dot src | dot -T svg > dependencygraph.svg",
    "cloc": "npx cloc src",
    "gen-tailwind": "npx tailwindcss -i ./src/globals.css -o ./cypress/plugins/globals.css",
    "cypress": "cypress open",
    "cypress:headless": "cypress run",
    "cypress:unit": "npm run gen-tailwind && cypress open-ct",
    "e2e": "start-server-and-test dev http://localhost:3000 cypress",
    "e2e:headless": "start-server-and-test dev http://localhost:3000 cypress:headless",
    "codegen": "graphql-codegen",
    "nyc": "npx nyc report --reporter=text-summary"
  },
